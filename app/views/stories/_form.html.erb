<%= simple_form_for(@story) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :location %>
    <%= f.input :url_1 %>
    <%= f.input :url_2 %>
    <%= f.input :course %>
    <%= f.input :summary %>
    <%= f.input :summary_url %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
