<p id="notice"><%= notice %></p>

<h1>Stories</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Location</th>
      <th>Url 1</th>
      <th>Url 2</th>
      <th>Course</th>
      <th>Summary</th>
      <th>Summary url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stories.each do |story| %>
      <tr>
        <td><%= story.title %></td>
        <td><%= story.location %></td>
        <td><%= story.url_1 %></td>
        <td><%= story.url_2 %></td>
        <td><%= story.course %></td>
        <td><%= story.summary %></td>
        <td><%= story.summary_url %></td>
        <td><%= link_to 'Show', story %></td>
        <td><%= link_to 'Edit', edit_story_path(story) %></td>
        <td><%= link_to 'Destroy', story, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Story', new_story_path %>
