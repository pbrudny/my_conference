<body>
<header class="row">
  <a href="/">

  <div class="col-3">
    <div class="circle">
      <br />
      Kickstart
      <br />
      Polska
      <br />
      2017
    </div>
  </div>
  </a>
  <div class="col-5 links">
    <%= link_to 'Wsparcie', root_path %>
    <%= link_to 'Szczegóły', static_pages_details_path %>
